{
  "name": "nanomorph",
  "version": "3.0.1",
  "description": "Hyper fast diffing algorithm for real DOM nodes",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps && browserify test.js | tape-run",
    "test:fast": "browserify test.js | tape-run",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover tape-run test.js",
    "derp": "tape-run index.js"
  },
  "repository": "yoshuawuyts/nanomorph",
  "keywords": [
    "dom",
    "diff",
    "tiny",
    "morphdom",
    "choo",
    "bel",
    "yo-yo"
  ],
  "license": "MIT",
  "dependencies": {
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "bel": "^4.4.3",
    "browserify": "^14.1.0",
    "dependency-check": "^2.5.1",
    "istanbul": "^0.4.4",
    "math-random-seed": "^1.0.0",
    "standard": "^7.1.2",
    "tape": "^4.6.0",
    "tape-run": "^3.0.0"
  }
}
